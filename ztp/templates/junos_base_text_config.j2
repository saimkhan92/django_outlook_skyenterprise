version 15.1R6.7;
system {
    host-name {{ hostname }};
    root-authentication {
        encrypted-password "{{ encrypted_pwd }}"; ## SECRET-DATA
    }
    name-server {
        {{ nameserver }};
    }
    login {
        user skyenterprise {
            uid 2000;
            class super-user;
            authentication {
                encrypted-password "{{ skyent_pwd }}"; ## SECRET-DATA
            }
        }
    }
    services {
        ssh;
        netconf {
            ssh;
        }
        outbound-ssh {
            client skyenterprise-ncd01 {
                device-id {{ ncd01_device_id }};
                secret "{{ ncd01_secret }}"; ## SECRET-DATA
                keep-alive {
                    retry 3;
                    timeout 5;
                }
                services netconf;
                skyent-ncd01.juniper.net {
                    port 4087;
                    retry 1000;
                    timeout 60;
                }
            }
            client skyenterprise-ncd02 {
                device-id {{ ncd02_device_id }};
                secret "{{ ncd02_secret }}"; ## SECRET-DATA
                keep-alive {
                    retry 3;
                    timeout 5;
                }
                services netconf;
                skyent-ncd02.juniper.net {
                    port 4087;
                    retry 1000;
                    timeout 60;
                }
            }
        }
        dhcp {
            traceoptions {
                file dhcp_logfile;
                level all;
                flag all;
            }
        }
    }
    syslog {
        user * {
            any emergency;
        }
        file messages {
            any notice;
            authorization info;
        }
        file interactive-commands {
            interactive-commands any;
        }
    }
}
chassis {
    auto-image-upgrade;
}
